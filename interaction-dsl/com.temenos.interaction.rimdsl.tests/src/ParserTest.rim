rim ParserTest {

events
	N POST // New
	P PUT // Pay
	U PUT // Used
	C DELETE // Cancel
	GET GET
end

commands
	t24Get z
	t24Delete z
end

initial resource demo_initial {
	type: item
	entity: HOME
	view { t24Get }
	GET -> demo_tickets
}

resource demo_tickets {
	type: collection
	entity: TICKET
	view { t24Get }
	GET -> demo_aticket
	C *-> demo_deleted
}

resource demo_aticket {
	type: item
	entity: TICKET
	view { t24Get }
	C -> demo_deleted
}

resource demo_deleted {
	type: item
	entity: TICKET
	actions {t24Delete}
	GET -> demo_tickets
}

}