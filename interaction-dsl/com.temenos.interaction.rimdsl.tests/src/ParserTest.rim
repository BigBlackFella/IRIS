events
	N POST // New
	P PUT // Pay
	U PUT // Used
	C DELETE // Cancel
end

commands
	t24Delete z
end

initial state demo_initial
	item HOME
	GET -> demo_tickets
end

state demo_tickets
	collection TICKET
	GET -> demo_aticket
	DELETE *-> demo_deleted
end

state demo_aticket
	item TICKET
	DELETE -> demo_deleted
end

state demo_deleted
	item TICKET
	actions {t24Delete}
	GET --> demo_tickets
end