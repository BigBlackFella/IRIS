domain TestDomain {
    use Common.Global.*
    use ONE.*
    
    rim ONE {
        commands
             NoopGET
        end
        initial resource A {
             type: collection
             entity: ENTITY
             view { NoopGET }
             GET -> TestDomain.TWO.B
        }
    }
    rim TWO {
        commands
             NoopGET
        end
        initial resource B {
             type: collection
             entity: ENTITY
             view { NoopGET }
             GET -> ONE.A
             GET -> A
        }
    }
}