<Metadata ModelName="InteractionNote" Version="1.0" 
          xmlns="http://iris.temenos.com/metadata.xsd"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://iris.temenos.com metadata.xsd">
    <Entity Name="ServiceDocument"></Entity>
    <Entity Name="Metadata"></Entity>
    <Entity Name="Note">
        <Property Name="id">
            <Term Name="TERM_MANDATORY">true</Term>
            <Term Name="TERM_VALUE_TYPE">INTEGER_NUMBER</Term>
            <Term Name="TERM_ID_FIELD">true</Term>
        </Property>
        <Property Name="body">
            <Term Name="TERM_MANDATORY">false</Term>
            <Term Name="TERM_VALUE_TYPE">STRING</Term>
        </Property>
        <Property Name="personId" />
    </Entity>
	<Entity Name="Person">
        <Property Name="id">
            <Term Name="TERM_MANDATORY">true</Term>
            <Term Name="TERM_VALUE_TYPE">INTEGER_NUMBER</Term>
            <Term Name="TERM_ID_FIELD">true</Term>
        </Property>
        <Property Name="name">
            <Term Name="TERM_MANDATORY">false</Term>
            <Term Name="TERM_VALUE_TYPE">STRING</Term>
        </Property>
	</Entity>
</Metadata>
