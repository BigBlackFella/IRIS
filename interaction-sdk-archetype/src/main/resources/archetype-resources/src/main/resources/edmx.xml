<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="1.0" xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx" xmlns:d="http://schemas.microsoft.com/ado/2007/08/dataservices" xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata">
    <edmx:DataServices m:DataServiceVersion="1.0">
        <Schema xmlns="http://schemas.microsoft.com/ado/2006/04/edm" Namespace="${groupId}.${artifactId}">
            <EntityType Name="Flight">
                <Key>
                    <PropertyRef Name="flightID"></PropertyRef>
                </Key>
                <Property Name="flightID" Type="Edm.Int64" Nullable="false"></Property>
                <Property Name="takeoffTime" Type="Edm.DateTime" Nullable="true"></Property>
                <NavigationProperty Name="flightSchedule" Relationship="${groupId}.${artifactId}.FK_Flight_FlightSchedule" FromRole="Flight" ToRole="FlightSchedule"></NavigationProperty>
            </EntityType>
            <EntityType Name="Airport">
                <Key>
                    <PropertyRef Name="code"></PropertyRef>
                </Key>
                <Property Name="name" Type="Edm.String" Nullable="true"></Property>
                <Property Name="code" Type="Edm.String" Nullable="false"></Property>
                <Property Name="country" Type="Edm.String" Nullable="true"></Property>
            </EntityType>
            <EntityType Name="BadAirport" BaseType="${groupId}.${artifactId}.Airport">
                <Property Name="rating" Type="Edm.Int32" Nullable="false"/>
                <Property Name="prop2" Type="Edm.String" Nullable="false"/>
            </EntityType>
            <EntityType Name="FlightSchedule">
                <Key>
                    <PropertyRef Name="flightScheduleID"></PropertyRef>
                </Key>
                <Property Name="arrivalAirportCode" Type="Edm.String" Nullable="true" MaxLength="255"></Property>
                <Property Name="flightScheduleID" Type="Edm.Int64" Nullable="false"></Property>
                <Property Name="arrivalTime" Type="Edm.Time" Nullable="true"></Property>
                <Property Name="flightNo" Type="Edm.String" Nullable="true" MaxLength="255"></Property>
                <Property Name="firstDeparture" Type="Edm.DateTime" Nullable="true"></Property>
                <Property Name="departureTime" Type="Edm.Time" Nullable="true"></Property>
                <Property Name="departureAirportCode" Type="Edm.String" Nullable="true" MaxLength="255"></Property>
                <Property Name="lastDeparture" Type="Edm.DateTime" Nullable="true"></Property>
                <NavigationProperty Name="departureAirport" Relationship="${groupId}.${artifactId}.FK_FlightSchedule_Airport" FromRole="FlightSchedule" ToRole="Airport"></NavigationProperty>
                <NavigationProperty Name="arrivalAirport" Relationship="${groupId}.${artifactId}.FK_FlightSchedule_Airport_1" FromRole="FlightSchedule" ToRole="Airport"></NavigationProperty>
            </EntityType>
            <EntityType Name="SubFlightSchedule" BaseType="${groupId}.${artifactId}.FlightSchedule">
                <Property Name="prop3" Type="Edm.String" Nullable="false"/>
                <Property Name="prop4" Type="Edm.String" Nullable="false"/>
                <Property Name="prop5" Type="Edm.String" Nullable="false"/>
            </EntityType>
             <EntityType Name="SubSubFlightSchedule" BaseType="${groupId}.${artifactId}.SubFlightSchedule">
                <Property Name="prop6" Type="Edm.String" Nullable="false"/>
                <Property Name="prop7" Type="Edm.String" Nullable="false"/>
                <Property Name="prop8" Type="Edm.String" Nullable="false"/>
                <Property Name="prop9" Type="Edm.String" Nullable="false"/>
                <NavigationProperty Name="backupSchedule" Relationship="${groupId}.${artifactId}.BackupSchedule"
                    FromRole="PrimaryFlightSchedule" ToRole="BackupFlightSchedule"></NavigationProperty>
            </EntityType>
            <Association Name="FK_Flight_FlightSchedule">
                <End Role="Flight" Type="${groupId}.${artifactId}.Flight" Multiplicity="*"></End>
                <End Role="FlightSchedule" Type="${groupId}.${artifactId}.FlightSchedule" Multiplicity="0..1"></End>
            </Association>
            <Association Name="FK_FlightSchedule_Airport">
                <End Role="FlightSchedule" Type="${groupId}.${artifactId}.FlightSchedule" Multiplicity="*"></End>
                <End Role="Airport" Type="${groupId}.${artifactId}.Airport" Multiplicity="0..1"></End>
            </Association>
            <Association Name="FK_FlightSchedule_Airport_1">
                <End Role="FlightSchedule" Type="${groupId}.${artifactId}.FlightSchedule" Multiplicity="*"></End>
                <End Role="Airport" Type="${groupId}.${artifactId}.Airport" Multiplicity="0..1"></End>
            </Association>
            <Association Name="BackupSchedule">
                <End Role="PrimaryFlightSchedule" Type="${groupId}.${artifactId}.FlightSchedule" Multiplicity="1"></End>
                <End Role="BackupFlightSchedule" Type="${groupId}.${artifactId}.FlightSchedule" Multiplicity="1"></End>
            </Association>
        </Schema>
        <Schema xmlns="http://schemas.microsoft.com/ado/2006/04/edm" Namespace="AirlineContainer">
            <EntityContainer Name="AirlineEntities" m:IsDefaultEntityContainer="true">
                <EntitySet Name="Flight" EntityType="${groupId}.${artifactId}.Flight"></EntitySet>
                <EntitySet Name="Airport" EntityType="${groupId}.${artifactId}.Airport"></EntitySet>
                <EntitySet Name="BadAirport" EntityType="${groupId}.${artifactId}.BadAirport"></EntitySet>
                <EntitySet Name="FlightSchedule" EntityType="${groupId}.${artifactId}.FlightSchedule"></EntitySet>
                <EntitySet Name="SubFlightSchedule" EntityType="${groupId}.${artifactId}.SubFlightSchedule"></EntitySet>
                <EntitySet Name="SubSubFlightSchedule" EntityType="${groupId}.${artifactId}.SubSubFlightSchedule"></EntitySet>

                <AssociationSet Name="FK_Flight_FlightSchedule" Association="${groupId}.${artifactId}.FK_Flight_FlightSchedule">
                    <End Role="Flight" EntitySet="Flight"></End>
                    <End Role="FlightSchedule" EntitySet="FlightSchedule"></End>
                </AssociationSet>
                <AssociationSet Name="FK_FlightSchedule_Airport" Association="${groupId}.${artifactId}.FK_FlightSchedule_Airport">
                    <End Role="FlightSchedule" EntitySet="FlightSchedule"></End>
                    <End Role="Airport" EntitySet="Airport"></End>
                </AssociationSet>
                <AssociationSet Name="FK_FlightSchedule_Airport_1" Association="${groupId}.${artifactId}.FK_FlightSchedule_Airport_1">
                    <End Role="FlightSchedule" EntitySet="FlightSchedule"></End>
                    <End Role="Airport" EntitySet="Airport"></End>
                </AssociationSet>
                <AssociationSet Name="BackupSchedule" Association="${groupId}.${artifactId}.BackupSchedule">
                    <End Role="PrimaryFlightSchedule" EntitySet="FlightSchedule"></End>
                    <End Role="BackupFlightSchedule" EntitySet="FlightSchedule"></End>
                </AssociationSet>
            </EntityContainer>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>