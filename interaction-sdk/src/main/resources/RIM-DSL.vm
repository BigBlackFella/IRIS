	path "/${rsm.getEntityName()}({id})/${transition.getTargetStateName()}"
	urispec NavProperty "/${rsm.getEntityName()}"
	GET -> ${transition.getTargetRsmStateName()} id=${transition.getTargetResourceStateMachine().getMappedEntityProperty()}
#end
end
