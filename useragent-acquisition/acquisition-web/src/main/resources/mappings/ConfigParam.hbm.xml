<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.temenos.ebank.domain.ConfigParam" table="CONFIG_PARAM" lazy="false">
		<cache usage="read-write" />
        <id name="codeParam" type="string">
            <column name="CODE_PARAM" length="50"/>
        </id>  
		<property name="valueParam" type="string" column="VALUE_PARAM" />
		<property name="typeParam" type="string" column="TYPE_PARAM" />
	</class>
</hibernate-mapping>