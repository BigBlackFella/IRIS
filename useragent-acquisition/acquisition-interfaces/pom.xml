<?xml version="1.0"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>temenos</groupId>
	<artifactId>acquisition-interfaces</artifactId>
	<packaging>jar</packaging>
	<version>1.0-SNAPSHOT</version>
	<name>Acquisition Interfaces</name>
	<description>
		Temenos Acquisition front end
	</description>
	<url>http://www.temenos.com</url>
	<inceptionYear>2011</inceptionYear>
	<organization>
		<name>Temenos</name>
		<url>http://www.temenos.com</url>
	</organization>

	<properties>
		<slf4j.version>1.6.1</slf4j.version>
	</properties>
	
	<dependencies>
		<dependency>
			<groupId>commons-logging</groupId>
			<artifactId>commons-logging</artifactId>
			<version>1.1.1</version>
		</dependency>
	</dependencies>   

	<parent>
		<groupId>temenos</groupId>
		<artifactId>acquisition</artifactId>
		<version>1.0-SNAPSHOT</version>  
		<relativePath>../acquisition-web/parentpom</relativePath>
	</parent>

	<build>
		<finalName>acquisition</finalName>
		<resources>
			<resource>
				<filtering>false</filtering>
				<directory>src/main/java</directory>
				<includes>
					<include>**</include>
				</includes>
				<excludes>
					<exclude>**/*.java</exclude>
				</excludes>
			</resource>
		</resources>
		<plugins>
		</plugins>
	</build>
</project>
