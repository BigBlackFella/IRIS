<?xml version="1.0" encoding="UTF-8"?>
<edmx:Edmx Version="1.0" xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx"
xmlns:d="http://schemas.microsoft.com/ado/2007/08/dataservices"
xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata">
    <edmx:DataServices m:DataServiceVersion="1.0">
        <Schema Namespace="AirlineContainer" xmlns="http://schemas.microsoft.com/ado/2006/04/edm">
            <EntityContainer Name="AirlineEntities" m:IsDefaultEntityContainer="true">
                <EntitySet Name="Airport" EntityType="AirlineModel.Airport"/>
                <EntitySet Name="FlightSchedule" EntityType="AirlineModel.FlightSchedule"/>
                <EntitySet Name="Flight" EntityType="AirlineModel.Flight"/>
                <AssociationSet Name="FK_FlightSchedule_Airport"
                Association="AirlineModel.FK_FlightSchedule_Airport">
                    <End Role="FlightSchedule" EntitySet="FlightSchedule"/>
                    <End Role="Airport" EntitySet="Airport"/>
                </AssociationSet>
                <AssociationSet Name="FK_FlightSchedule_Airport_1"
                Association="AirlineModel.FK_FlightSchedule_Airport_1">
                    <End Role="FlightSchedule" EntitySet="FlightSchedule"/>
                    <End Role="Airport" EntitySet="Airport"/>
                </AssociationSet>
                <AssociationSet Name="FK_Flight_FlightSchedule"
                Association="AirlineModel.FK_Flight_FlightSchedule">
                    <End Role="Flight" EntitySet="Flight"/>
                    <End Role="FlightSchedule" EntitySet="FlightSchedule"/>
                </AssociationSet>
            </EntityContainer>
        </Schema>
        <Schema Namespace="AirlineModel" xmlns="http://schemas.microsoft.com/ado/2006/04/edm">
            <EntityType Name="Airport">
                <Key>
                    <PropertyRef Name="code"/>
                </Key>
                <Property Name="name" Type="Edm.String" Nullable="true"/>
                <Property Name="code" Type="Edm.String" Nullable="false"/>
                <Property Name="country" Type="Edm.String" Nullable="true"/>
            </EntityType>
            <EntityType Name="FlightSchedule">
                <Key>
                    <PropertyRef Name="flightScheduleID"/>
                </Key>
                <Property Name="arrivalAirportCode" Type="Edm.String"
                Nullable="true" MaxLength="255"/>
                <Property Name="flightScheduleID" Type="Edm.Int64"
                Nullable="false"/>
                <Property Name="arrivalTime" Type="Edm.Time"
                Nullable="true"/>
                <Property Name="flightNo" Type="Edm.String"
                Nullable="true" MaxLength="255"/>
                <Property Name="firstDeparture" Type="Edm.DateTime"
                Nullable="true"/>
                <Property Name="departureTime" Type="Edm.Time"
                Nullable="true"/>
                <Property Name="departureAirportCode" Type="Edm.String"
                Nullable="true" MaxLength="255"/>
                <Property Name="lastDeparture" Type="Edm.DateTime"
                Nullable="true"/>
                <NavigationProperty Name="departureAirport"
                Relationship="AirlineModel.FK_FlightSchedule_Airport"
                FromRole="FlightSchedule" ToRole="Airport"/>
                <NavigationProperty Name="arrivalAirport" Relationship="AirlineModel.FK_FlightSchedule_Airport_1"
                FromRole="FlightSchedule" ToRole="Airport"/>
            </EntityType>
            <EntityType Name="Flight">
                <Key>
                    <PropertyRef Name="flightID"/>
                </Key>
                <Property Name="flightID" Type="Edm.Int64" Nullable="false"/>
                <Property Name="takeoffTime" Type="Edm.DateTime"
                Nullable="true"/>
                <NavigationProperty Name="flightSchedule" Relationship="AirlineModel.FK_Flight_FlightSchedule"
                FromRole="Flight" ToRole="FlightSchedule"/>
            </EntityType>
            <Association Name="FK_FlightSchedule_Airport">
                <End Role="FlightSchedule" Type="AirlineModel.FlightSchedule"
                Multiplicity="*"/>
                <End Role="Airport" Type="AirlineModel.Airport"
                Multiplicity="0..1"/>
            </Association>
            <Association Name="FK_FlightSchedule_Airport_1">
                <End Role="FlightSchedule" Type="AirlineModel.FlightSchedule"
                Multiplicity="*"/>
                <End Role="Airport" Type="AirlineModel.Airport"
                Multiplicity="0..1"/>
            </Association>
            <Association Name="FK_Flight_FlightSchedule">
                <End Role="Flight" Type="AirlineModel.Flight"
                Multiplicity="*"/>
                <End Role="FlightSchedule" Type="AirlineModel.FlightSchedule"
                Multiplicity="0..1"/>
            </Association>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>
