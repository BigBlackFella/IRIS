<?xml version="1.0" encoding="utf-8"?>
<edmx:Edmx Version="1.0" xmlns:edmx="http://schemas.microsoft.com/ado/2007/06/edmx" xmlns:d="http://schemas.microsoft.com/ado/2007/08/dataservices" xmlns:m="http://schemas.microsoft.com/ado/2007/08/dataservices/metadata">
    <edmx:DataServices m:DataServiceVersion="1.0">
        <Schema xmlns="http://schemas.microsoft.com/ado/2006/04/edm" Namespace="com.temenos.interaction.example.mashup.twitter">
            <EntityContainer Name="Entities" m:IsDefaultEntityContainer="true">
                <EntitySet Name="user" EntityType="com.temenos.interaction.example.mashup.twitter.User"/>
                <EntitySet Name="Timeline" EntityType="com.temenos.interaction.example.mashup.twitter.Tweet"/>
            </EntityContainer>
            <EntityType Name="User">
                <Key>
                    <PropertyRef Name="userID"></PropertyRef>
                </Key>
                <Property Name="userID" Type="Edm.String" Nullable="false"></Property>
                <Property Name="fullName" Type="Edm.String" Nullable="false"></Property>
                <Property Name="twitterHandle" Type="Edm.String" Nullable="true"></Property>
            </EntityType>
            <EntityType Name="Tweet">
                <Key>
                    <PropertyRef Name="username"></PropertyRef>
                </Key>
                <Property Name="message" Type="Edm.String" Nullable="false"></Property>
                <Property Name="geolocation" Type="Edm.String" Nullable="true"></Property>
            </EntityType>
        </Schema>
    </edmx:DataServices>
</edmx:Edmx>